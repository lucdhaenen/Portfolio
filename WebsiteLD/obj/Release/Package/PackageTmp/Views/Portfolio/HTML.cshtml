@{
    ViewBag.Title = "HTML";
}
<div id="container" class="clearfix">
    <div id="voorbeelden" class="clearfix">
        <div id="partial" class="clearfix">
            @Html.Partial("Voorbeelden")
        </div>
        <div id="inhoud" class="clearfix">
            <div id="inhoudH4" class="clearfix">
                <div id="titel" class="left">
                    <h4>HTML 5 / CSS 3</h4>
                </div>
                <div id="downloadVoorbeeld" class="clearfix">
                    <ul>
                        <li>
                            <div id="wrapperDownload" class="clearfix">
                                <div id="downloadImg">
                                    <img src="~/Content/images/download.png" />
                                </div>
                                <div id="downloadText">
                                    <a href="~/Content/files/HTML.pdf" target="_blank">Inhoudsopgave projectenboek</a>
                                </div>
                            </div>
                        </li>
                    </ul>
                </div>
            </div>
            <div id="oefeningen" class="voorbeelden">
                <h1>Oefeningen</h1>
                <div id="screens">                    
                    <div id="imgModules">
                        <a href="@Url.Action("HTMLdeGazet", "Modules")"><img src="~/Content/images/rsz_degazet.jpg" /></a>
                    </div>
                </div>
            </div>
            <div id="testen" class="voorbeelden">
                <h1>Test</h1>
                <div id="screens">
                    <div id="imgModules">
                        <a href="@Url.Action("Bridges", "Modules")"><img src="~/Content/images/rsz_Bridges.png" /></a>
                    </div>
                </div>
            </div>
            <br />
            <br />
            <p>
                Omdat deze pagina's nog volop in ontwikkeling zijn, zullen stelselmatig oefeningen en de betreffende
                links actief worden.
            </p>
        </div>
    </div>
</div>
